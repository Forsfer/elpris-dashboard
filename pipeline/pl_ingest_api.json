{
	"name": "pl_ingest_api",
	"properties": {
		"description": "Pipelinen tar in json-data från elpris-APIet och sparar det i en fil för dagens datum i Data Lake:n. En trigger exekuterar pipelinen 5 minuter efter varje timme och skriver då över tidigare innehåll i filen så att den inkluderar senaste datan.",
		"activities": [
			{
				"name": "copy_api_prices_to_datalake",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ds_elpris_api",
						"type": "DatasetReference",
						"parameters": {
							"relativePath": {
								"value": "@concat(\n  formatDateTime(utcNow(),'yyyy'),\n  '/',\n  formatDateTime(utcNow(),'MM'),\n  '-',\n  formatDateTime(utcNow(),'dd'),\n  '_SE3.json'\n)",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_raw_elpris",
						"type": "DatasetReference",
						"parameters": {
							"folderPath": {
								"value": "@concat(\n  'raw/elpriser/'\n)\n",
								"type": "Expression"
							},
							"fileName": {
								"value": "@concat(\n  'prices_',\n  formatDateTime(addHours(utcNow(), -1), 'yyyyMMdd'),\n  '.json'\n)",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2026-02-08T09:06:25Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}